.has-tooltip{ class: "limit-box limit-box-#{objects} prepend-left-5", data: { title: "Project has too many #{label_for_message} to search"} }
  .limit-icon
    - if objects == :branch
      = icon('code-fork')
    - else
      = icon('tag')
  .limit-message
    %span #{label_for_message.capitalize} unavailable
